# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore db config
/config/database.yml

# Ignore secrets
/config/secrets.yml

/dump.rdb

/node_modules
/app/assets/javascripts/react_bundle.js
/app/assets/stylesheets/react_bundle.css
